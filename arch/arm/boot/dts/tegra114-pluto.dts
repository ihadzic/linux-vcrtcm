/dts-v1/;

/include/ "tegra114.dtsi"

/ {
	model = "NVIDIA Tegra114 Pluto evaluation board";
	compatible = "nvidia,pluto", "nvidia,tegra114";

	memory {
		reg = <0x80000000 0x40000000>;
	};

	serial@70006300 {
		status = "okay";
		clock-frequency = <408000000>;
	};

	pmc {
		nvidia,invert-interrupt;
	};
};
