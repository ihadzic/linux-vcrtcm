/dts-v1/;
/include/ "sun5i-a13.dtsi"

/ {
	model = "Olimex A13-Olinuxino";
	compatible = "olimex,a13-olinuxino", "allwinner,sun5i-a13";

	chosen {
		bootargs = "earlyprintk console=ttyS0,115200";
	};

	soc {
		uart1: uart@01c28400 {
			status = "okay";
		};
	};
};
